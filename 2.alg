algoritmo "Exercicio2"
var
    nome: caractere
    idade, contador: inteiro

inicio
    contador <- 0
    faca
        escreva("Digite o nome da pessoa: ")
        leia(nome)
        escreva("Digite a idade da pessoa: ")
        leia(idade)
        contador <- contador + 1
    enquanto contador < 3
fimalgoritmo



